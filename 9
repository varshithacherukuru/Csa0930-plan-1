public class Main {
    public static void main(String[] args) {
        // Defining three numbers
        int num1 = 10;
        int num2 = 20;
        int num3 = 30;

        // Finding the greatest of three numbers using ternary operators
        int max = (num1 > num2) ? (num1 > num3 ? num1 : num3) : (num2 > num3 ? num2 : num3);

        // Printing the greatest of three numbers
        System.out.println("The greatest of three numbers is: " + max);
    }
}
